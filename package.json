    "type": "module",
    "main": "app.js",
    "scripts": {
        "start": "node server/app",
        "prettier:fix": "prettier 'server/**/*.js' --write",
        "eslint:fix": "eslint .client/src --fix",
        "codeStyle:fix": "npm run prettier:fix && npm run eslint:fix",
        "clear-all": "rimraf ./build ./node_modules",
