        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "axios": "^0.25.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^14.3.2",
        "express": "^4.17.3",
        "express-rate-limit": "^6.3.0",
        "express-session": "^1.17.2",
        "mongoose": "^6.1.8",
        "morgan": "^1.10.0",
        "nanoid": "^3.3.1",
        "node-uri": "^1.1.1",
        "nodemon": "^2.0.15",
        "passport": "^0.5.2",
        "passport-github2": "^0.1.12",
        "passport-google-oauth20": "^2.0.0",
        "passport-twitter": "^1.0.4",
